{
  "name": "foobar",
  "type": "module",
  "imports": {
    "#*": "./*.js"
  },
  "scripts": {
    "start": "node --env-file=.env server/server.js",
    "dev": "node --watch --env-file=.env server/server.js",
    "test": "vitest",
