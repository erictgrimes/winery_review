  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.1",
    "pg": "^8.14.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "vitest": "^3.2.4"
  }
}
